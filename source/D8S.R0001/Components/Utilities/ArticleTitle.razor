@*Namespace specified in directory-level _Imports.razor.*@


<h1 class="mt-8">
    <span class="mt-2 block text-3xl leading-8 font-bold tracking-tight text-gray-900 sm:text-4xl">@this.Text</span>
</h1>


@code{
    /// <summary>
    /// The text to be shown for the title.
    /// </summary>
    [Parameter]
    public string Text { get; set; }
}
